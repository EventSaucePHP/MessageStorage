            "target": "git@github.com:EventSaucePHP/MessageRepositoryTableSchema.git"
        },
        {
            "name": "outbox-testing",
            "directory": "src/UuidEncoding",
            "target": "git@github.com:EventSaucePHP/UuidEncoding.git"
        }
