{
    "dependencies-must-satisfy": {
        "eventsauce/eventsauce": "1.0.0",
        "eventsauce/message-outbox": "0.1.0"
    },
    "sub-splits": [
        {
            "name": "core",
            "directory": "src/MessageOutbox",
            "target": "git@github.com:EventSaucePHP/MessageOutbox.git"
        },
        {
            "name": "illuminate-outbox",
            "directory": "src/IlluminateOutbox",
            "target": "git@github.com:EventSaucePHP/MessageOutboxForIlluminate.git"
        },
        {
            "name": "doctrine-outbox",
            "directory": "src/DoctrineOutbox",
